<h2>Search Movies</h2>
<form action="/movies" methode="GET">
  <label>Title</label>
  <input type="text" name="title" value="<%= searchOptions.title %>">
  <button type="submit">Search</button>
</form>
<br>
<% movies.forEach(movie => { %> 
<div>
  <%= movie.title %>
  <br>
  <img src="<%= movie.coverURL %>">
</div>
<% }) %>