<h2 class="page-header">Search TV Shows</h2>
<form action="/tvshows" methode="GET">
  <input type="text" name="title" value="<%= searchOptions.title %>" placeholder="Title (e.g. My Wife and Kids)" class="input search-input">
  <button class="btn-primary search-btn" type="submit">Search</button>
</form>
<br>
<div class="show-all">
  <% tvshows.forEach(tvshow => { %> 
    <div>
      <a href="tvshows/<%= tvshow.id %>""><img class="cover-img-all" src="<%= tvshow.coverURL %>"></a>
    </div>
    <% }) %>
</div>
